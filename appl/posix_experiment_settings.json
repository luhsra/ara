{
	"POSIXInit": {
		"system_profile": "POSIX",
		"enable_musl_syscalls": true,
        "enable_missing_interaction_count": true
	},

	"RemoveSysfuncBody": {
		"drop_llvm_suffix": false
	},

	"SysFuncts": {
		"no_stubs": true
	},

	"SystemRelevantFunctions": {
		"no_stubs": true
	},

    "IRReader": {
        "no_sysfunc_body": true
    },

	"ICFG": {
		"dump": true
	},

	"logger": {
		"POSIX": "debug"
	}
}
